[program:{{name}}]
command = {{gunicorn_start_sh_path}}                   ; Command to start app
user = {{robot_name}}                                  ; User to run as
stdout_logfile = {{supervisor_log}}   ; Where to write log messages
redirect_stderr = true                                                ; Save stderr in the same log
environment=LANG=en_US.UTF-8,LC_ALL=en_US.UTF-8                       ; Set UTF-8 as default encoding
